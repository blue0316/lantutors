{
  "name": "lantutors-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "nodemon -r dotenv/config src/server.js dotenv_config_path=./.env",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "test": "jest --testTimeout=30000  --watchAll --runInBand ",
    "pretest": "cross-env NODE_ENV=test npm run db:reset:test",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "db:reset:test": "cross-env NODE_ENV=test npx sequelize-cli db:drop && cross-env NODE_ENV=test npx sequelize-cli db:create && cross-env NODE_ENV=test npx sequelize-cli db:migrate"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/israelias/lantutors.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/israelias/lantutors/issues"
  },
  "homepage": "https://github.com/israelias/lantutors#readme",
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "@babel/runtime": "^7.13.10",
    "babel-loader": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.8",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
